TELEGRAM_BOT_TOKEN=
OPENAI_API_KEY=
# Use WOODY_DB_PATH (absolute path) so Woody and dashboard share same DB. Avoid APP_DB_PATH=./app.db - it resolves to cwd and causes "Unknown approval ID" when Woody runs from dashboard/
# WOODY_DB_PATH=/path/to/woody/app.db
FILES_SANDBOX_DIR=./sandbox_files

# OpenTelemetry (optional)
# OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
# OTEL_CONSOLE_EXPORT=true

# Calendar timezone (set to your timezone so events show at the right time)
# CALENDAR_TIMEZONE=America/Los_Angeles

# Memory agent runs nightly (UTC hour, default 3)
# MEMORY_AGENT_HOUR_UTC=3

# GitHub (optional)
# GITHUB_TOKEN=

# Dashboard auth (optional)
# DASHBOARD_USER=
# DASHBOARD_PASSWORD=

# Twilio (optional, for SMS via communications_send)
# TWILIO_ACCOUNT_SID=
# TWILIO_AUTH_TOKEN=
# TWILIO_PHONE_NUMBER=
